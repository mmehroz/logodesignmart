        itemsDesktopSmall : [979,3]
   
    });
   
  });

  $(function () {
   $("#owl-example").owlCarousel({
    transitionStyle: "fade",
     items: 1,
     animateOut: 'fadeOut',
     animateIn: 'fadeOut',
     loop: true,
     margin: 10,
     autoPlay:true,
     afterMove: moved,
   });



